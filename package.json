{
  "name": "rssarr",
  "version": "2.1.0",
  "private": "true",
  "description": "Rssarr, bridge between Anime RSS and Sonarr",
  "type": "module",
  "exports": "./server/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "homepage": "https://github.com/Izumiko/rssarr",
  "scripts": {
    "start": "bun server/index.js",
    "dev": "run-p dev:server dev:web",
    "dev:server": "bun --watch server/index.js",
    "dev:web": "vite",
    "build": "npm-run-all build:*",
    "build:web": "vite build --outDir dist/public",
    "build:server": "bun build server/index.js --target=node --minify --outfile=dist/index.mjs --define \"__dirname=.\" && cp node_modules/errorhandler/public/* dist/public/",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^6.5.0",
    "axios": "^1.8.2",
    "dotenv-cli": "^10.0.0",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "global-agent": "^3.0.0",
    "http-proxy-middleware": "^3.0.5",
    "json-server": "^0.17.4",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^1.0.0",
    "parse-torrent": "^11.0.18",
    "qs": "^6.14.0",
    "ra-data-json-server": "^5.10.1",
    "react": "^19.1.1",
    "react-admin": "^5.10.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "swr": "^2.3.4",
    "use-clipboard-copy": "^0.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.1"
  }
}
